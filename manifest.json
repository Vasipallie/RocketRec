{
  "manifest_version": 3,
  "name": "RocketRec",
  "version": "1.0.0",
  "description": "A google meet extension that records your meetings.",
  "icons": {
    "48": "assets/logo.png",
    "128": "assets/logo.png"
  }, 
  "action": {},  
  "permissions": [
    "storage",
     "activeTab",
    "scripting",
    "tabCapture"
  ],
  "background": { 
    "service_worker": "background.js" 
  },
  "host_permissions": [
     "<all_urls>" 
  ] 
} 

 
